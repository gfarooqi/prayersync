# Supabase Configuration
# Copy this file to .env and fill in your Supabase project details

# Supabase Project Settings (from your Supabase dashboard)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Environment
NODE_ENV=development

# Email Configuration (for transactional emails)
RESEND_API_KEY=your-resend-api-key-here
FROM_EMAIL=noreply@prayersync.com

# App Configuration
APP_URL=https://prayersync.com
APP_NAME=PrayerSync

# Analytics (optional)
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VERCEL_ANALYTICS_ID=your-vercel-analytics-id

# Security
SESSION_SECRET=your-session-secret-here
ENCRYPTION_KEY=your-encryption-key-here